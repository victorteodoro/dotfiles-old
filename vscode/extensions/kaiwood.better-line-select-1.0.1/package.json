{
	"name": "better-line-select",
	"displayName": "Better Line Select",
	"description": "Line selection on steroids",
	"version": "1.0.1",
	"publisher": "kaiwood",
	"license": "MIT",
	"bugs": "https://github.com/kaiwood/vscode-better-line-select/issues",
	"repository": {
		"type": "git",
		"url": "https://github.com/kaiwood/vscode-better-line-select"
	},
	"engines": {
		"vscode": "^1.5.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:better-line-select.downward",
		"onCommand:better-line-select.upward"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "better-line-select.downward",
				"title": "Select Line (downward)"
			},
			{
				"command": "better-line-select.upward",
				"title": "Select Line (upward)"
			}
		],
		"keybindings": [
			{
				"command": "better-line-select.downward",
				"key": "ctrl+l",
				"mac": "cmd+l",
				"when": "editorTextFocus"
			},
			{
				"command": "better-line-select.upward",
				"key": "ctrl+shift+l",
				"mac": "cmd+shift+l",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
		"vscode": "^1.0.0",
		"mocha": "^2.3.3",
		"@types/node": "^6.0.40",
		"@types/mocha": "^2.2.32"
	},
	"__metadata": {
		"id": "9c51dc38-4ae5-43ed-b921-60a8d379bebf",
		"publisherId": "9673635b-68b4-4497-a579-2447b4946e78",
		"publisherDisplayName": "Kai Wood"
	}
}