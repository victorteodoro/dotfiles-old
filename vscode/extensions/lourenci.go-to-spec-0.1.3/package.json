{
	"name": "go-to-spec",
	"displayName": "Go to Spec",
	"description": "Switch between the code and the spec file",
	"version": "0.1.3",
	"publisher": "Lourenci",
	"icon": "assets/icon.png",
	"homepage": "https://github.com/lourenci/go-to-spec",
	"repository": {
		"type": "git",
		"url": "https://github.com/lourenci/go-to-spec.git"
	},
	"bugs": {
		"url": "https://github.com/lourenci/go-to-spec/issues"
	},
	"engines": {
		"vscode": "^1.22.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"test",
		"spec",
		"jest",
		"rspec",
		"switcher",
		"switch"
	],
	"activationEvents": [
		"onCommand:extension.goToSpec"
	],
	"main": "./extension",
	"contributes": {
		"commands": [
			{
				"command": "extension.goToSpec",
				"title": "Go to Spec"
			}
		],
		"keybindings": [
			{
				"command": "extension.goToSpec",
				"key": "ctrl+shift+t",
				"mac": "cmd+shift+t",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^8.10.0",
		"standard": "^11.0.1",
		"vscode": "^1.1.6"
	},
	"__metadata": {
		"id": "3159f569-1cd8-4740-abf4-89d633e7fe6e",
		"publisherId": "82dbb3e9-ab7c-4ff0-8e78-97b5254151ff",
		"publisherDisplayName": "Lourenci"
	}
}